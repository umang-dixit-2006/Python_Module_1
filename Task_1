#Task 1

num_a = int(input("Enter the 1st number: "))
num_b = int(input("Enter the 2nd number: "))

add = int(num_a + num_b)
print("Addition: ",add)

sub = int(num_a - num_b)
print("subtraction: ",sub)

mul = num_a * num_b
print("Multiplication: ",mul)

div = float(num_a / num_b)
print("Division: ",div)
